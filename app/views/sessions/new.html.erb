<h3>New Session - User Login Page</h3>

<%= form_for :user do |u| %>
  <%= u.label :username %>
  <%= u.text_field :username %><br />
  <%= u.label :password %>  
  <%= u.password_field :password %><br />
  <%= u.submit "Submit" %>
<% end %>

<hr>

<%= link_to('Log in with Github!', '/auth/login') %>