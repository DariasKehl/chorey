<%=form_for @chore_list do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %> <br />
    <%= f.label :room %>
    <%= f.text_field :room %> <br /><br />>
    
    <%= f.fields_for :tasks do |tk| %>
        <%= tk.label :name %>
        <%= tk.text_field :name %>
        <%= tk.label :details %>
        <%= tk.text_field :details %>
        <%= tk.label :task_for %>
        <%= tk.text_field :task_for %>
    <% end %>
    
    <%= f.submit %>
    
<% end %>